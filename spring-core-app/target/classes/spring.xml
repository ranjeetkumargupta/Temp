<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->
	
	<bean id="empBean1" class="com.wipro.model.Employee">
		<constructor-arg  name="empno" value="1001"/>
		<constructor-arg  name="ename" value="Ranjeet"/>
		<constructor-arg  name="hiredate" value="2024-05-12"/>
		<constructor-arg  name="job" value="Trainee"/>
		<constructor-arg  name="sal" value="10000.0"/>
		<constructor-arg  name="deptno" value="10"/>
		
	</bean>
	
	<bean id="empBean2" class="com.wipro.model.Employee">
		<property name="empno" value="1002"/>
		<property name="ename" value="Raj"/>
		<property name="customHiredate" value="2024-05-11"/>
		<property name="job" value="Developer"/>
		<property name="sal" value="20000.0"/>
		<property name="deptno" value="20"/>
		
	
	</bean>

</beans>
